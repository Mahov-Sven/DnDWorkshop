class Primitive {
	private value;
	
	construct(value){
		this.set(value);
	}
	
	public operation noop get(){
		return this.value;
	}
	
	public operation = set(value){
		this.value = value;
	}
	
	public operation == equal(obj){
		return ((Primitive) obj).value == this.value;
	}
	
	public operation test boolValue(){
		return this.value;
	}
}